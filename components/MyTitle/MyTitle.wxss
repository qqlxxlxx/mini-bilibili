.my-title{
	display: flex;
	align-items: center;
	padding: 15rpx 20rpx;
	background-color: #fff;
	
}
.logo{
	flex: 7;
}
.logo image{
	width: 140rpx;
	height: 60rpx;
}
.search-icon{
	flex: 1;
	display: flex;
	justify-content: center;
	align-items: center;
}
.search-icon image{
	width: 44rpx;
	height: 44rpx;
}
.user-icon{
	flex: 2;
	display: flex;
	justify-content: center;
	align-items: center;
}
.user-icon image{
	width: 60rpx;
	height: 60rpx;
}
.down-app{
	flex: 3;
	background: #fb7299;
	font-size: 30rpx;
	color: #fff;
	display: flex;
	justify-content: center;
	align-items: center;
	border-radius: 10rpx;
	padding: 10rpx;
}