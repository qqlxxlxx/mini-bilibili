.nav{
	padding-left: 48rpx;
  margin-bottom: 10rpx;
  white-space: nowrap;
}
.nav-item{
  display: inline-block;
  margin-right: 70rpx;
	padding: 15rpx 0;
	font-size: 28rpx;
  color: #757575;
}
.nav-item.active{
	border-bottom: 5rpx solid #fb7299;
	color: #fb7299;
}

.slides{
	padding: 0 20rpx;
	margin-bottom: 30rpx;
}
.slides swiper{
	height: 220rpx;
	border-radius: 20rpx;
	overflow: hidden;
}
.slides swiper navigator{
	width: 100%;
	height: 100%;
}
.slides swiper navigator image{
	width: 100%;
	height: 100%;
}

.videos-wrap{
	display: flex;
	flex-wrap: wrap;
	padding: 0 10rpx;
}
.videos-wrap .video-item{
	width: 50%;
	padding: 0 10rpx;
	margin-bottom: 30rpx;
}
.videos-wrap .video-item .video-img{
	width: 100%;
	height: 212rpx;
	border-radius: 20rpx;
	overflow: hidden;
	position: relative;
}
.videos-wrap .video-item .video-img::after{
	content: "";
	display: block;
	width: 100%;
	height: 45rpx;
	position: absolute;
	bottom: 0;
	background: linear-gradient(to bottom, rgba(0,0,0,0), rgba(0,0,0,0.5));
}
.videos-wrap .video-item .video-img image{
	width: 100%;
	height: 100%;
	vertical-align: middle;
}
.videos-wrap .video-item .video-img .video-info{
	position: absolute;
	bottom: 5rpx;
	z-index: 5;
	margin-left: 20rpx;
	display: flex;
	justify-content: space-between;
}
.videos-wrap .video-item .video-img .video-info image{
  width: 30rpx;
}
.videos-wrap .video-item .video-img .video-info view{
  margin-right: 50rpx;
  padding-left: 40rpx;
	font-size: 24rpx;
	color: #fff;
}
.videos-wrap .video-item .video-img .video-info view:nth-child(1){
  background: url("data:image/svg+xml;base64,") no-repeat left center/28rpx;
}
.videos-wrap .video-item .video-img .video-info view:nth-child(2){
  background: url("data:image/svg+xml;base64,") no-repeat left center/28rpx;
}
.videos-wrap .video-item .video-desc{
	margin-top: 10rpx;
	font-size: 26rpx;
  color: #212121;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}